<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title>people</title>
        <link rel="icon" type="image/png" sizes="16x16" href="/images/favicon.png">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
        <link rel="stylesheet" href="https://use.fontawesome.com/releases/v5.3.1/css/all.css" integrity="sha384-mzrmE5qonljUremFsqc01SB46JvROS7bZs3IO2EmfFsd15uHvIt+Y8vEf7N7fWAU" crossorigin="anonymous">
        <link rel="stylesheet" type="text/css" href="/styles.css"/>
    </head>
    <body style="background-color: beige;">
      <a class="back_class" href="/classList/<%=userType%>">ClassList<i class="fas fa-chevron-circle-right"></i></a>
        <section id="class_menu" class="fixed-top">
            <div class="container">
                 
                      <ul class="navbar">
                        <li class="nav-item">
                          <a class="nav-link" href="/courseDetails/<%=courseId%>/<%=userType%>">Resources</a>
                        </li>
                        <li class="nav-item active">
                          <a class="nav-link " href="/people/<%=courseId%>/<%=userType%>">People</a>
                        </li>
                          <li class="nav-item">
                            <%if(userType=="created"){%>
                            <a class="nav-link" href="/grade/<%=courseId%>/all">Grade</a>
                            <%}else{%>
                              <a class="nav-link" href="/grade/<%=courseId%>/one">Grade</a>
                              <%}%>
                          </li>
                          
                        
                      </ul>
                    
            </div>
           </section>
           
           <section id="people">
            <div class="">
              <div class="teacher">
              <div class="teacher_heading">
              <h1>Teacher</h1>
              <hr/>
            </div>
            
              <p><span>
                <%if(findProfilePic){%>
                  <img class="profile_img" src="/<%=findProfilePic.pic_path%>"/>
                  <%}else{%>
                    <img class="profile_img" src="/uploads/profile_pic.jpg"/>
                    <%}%>
              </span><%=findClass.teacher_name%></p>
            </div>
            
            <div class="students">
              <div class="classmates_heading">
                <%if(userType=='enrolled'){%>
              <h1>Classmates</h1>
              <%}else{%>
                <h1>Students</h1>
                <%}%>
              <hr/>
            </div>
            <%if(typeof enrolledStudents!=='undefined'){%>
              <%enrolledStudents.forEach(function(student,index){%>
                <%if(userType=='enrolled'){%>
                  <%if(student.student_name!==userName){%>
              <p><span>
                <img class="profile_img" src="/<%=st_Profile[index]%>"/>
              </span><%=student.student_name%></p><hr/>
              <%}%>
              <%}else{%>
                <p><span>
                  <img class="profile_img" src="/<%=st_Profile[index]%>"/>
                </span><%=student.student_name%></p><hr/>
                <%}%>
              <%})%>
              <%}%>
            </div>
           </div>
          </section>
           <script src="https://code.jquery.com/jquery-3.4.1.slim.min.js" integrity="sha384-J6qa4849blE2+poT4WnyKhv5vZF5SrPo0iEjwBvKU7imGFAV0wwj1yYfoRSJoZ+n" crossorigin="anonymous"></script>
<script src="https://cdn.jsdelivr.net/npm/popper.js@1.16.0/dist/umd/popper.min.js" integrity="sha384-Q6E9RHvbIyZFJoft+2mJbHaEWldlvI9IOYy5n3zV9zzTtmI3UksdQRVvoxMfooAo" crossorigin="anonymous"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.min.js" integrity="sha384-wfSDF2E50Y2D1uUdj0O3uMBJnjuUD4Ih7YwaYd1iqfktj0Uod8GCExl3Og8ifwB6" crossorigin="anonymous"></script>
<script src="/client.js"></script>
    </body>
</html>